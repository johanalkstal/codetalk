{
  "name": "codetalk",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "pika-web --dest src/web_modules",
    "start": "lite-server --baseDir=src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "hybrids": "^2.0.2",
    "page": "^1.11.4",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "@pika/web": "^0.4.2",
    "eslint": "^5.16.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "lite-server": "^2.4.0",
    "prettier": "^1.16.4"
  }
}
